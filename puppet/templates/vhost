# <VirtualHost *:80>
# 	DocumentRoot /var/www/public
# 	
# 	<Directory />
# 		Options FollowSymLinks
# 		AllowOverride None
# 	</Directory>
# 	
# 	<Directory /var/www/>
# 		Options Indexes FollowSymLinks MultiViews
# 		AllowOverride All
# 		Order allow,deny
# 		allow from all
# 	</Directory>
# 
# 	ErrorLog /var/log/apache2/error.log
# 	LogLevel warn
# 	CustomLog /var/log/apache2/access.log combined
# </VirtualHost>

<VirtualHost *:80>
    DocumentRoot "/var/www/znanika-school/webroot"
    ServerName znanika-school
    ServerAlias www.znanika-school
    ErrorLog "logs/error.log"
    CustomLog "logs/access.log" combined
    <Directory "/var/www/znanika-school/webroot">
        
    </Directory>

    #php_value default_charset utf8
    #php_admin_value mbstring.func_overload 0
    #php_value mbstring.internal_encoding utf-8
</VirtualHost>

<VirtualHost *:80>
    DocumentRoot "/var/www/znanika-school/test"
    ServerName test.znanika-school
    ErrorLog "logs/error.log"
    CustomLog "logs/access.log" combined
    <Directory "/var/www/znanika-school/test">
        
    </Directory>

    #php_value default_charset utf8
    #php_admin_value mbstring.func_overload 0
    #php_value mbstring.internal_encoding utf-8
</VirtualHost>


